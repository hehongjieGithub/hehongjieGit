<view class="content"><image class="logo" src="../../../static/user_03.png"></image><text class="username">{{phone}}</text><view class="codeWrap"><input class="code" type="text" placeholder-class="place" placeholder="请输入验证码" data-event-opts="{{[['input',[['onCode',['$event']]]]]}}" value="{{code}}" bindinput="__e"/><button class="getCode" disabled="{{isDisable}}" data-event-opts="{{[['tap',[['gotCode',['$event']]]]]}}" bindtap="__e">{{getCode}}</button></view><view class="passWrap"><input class="password" type="{{passType}}" placeholder="请输入新密码" placeholder-class="place" data-event-opts="{{[['input',[['onPassword',['$event']]]]]}}" value="{{password}}" bindinput="__e"/><image class="password_isApprence" src="{{isSee}}" data-event-opts="{{[['tap',[['ImageChange',['$event']]]]]}}" bindtap="__e"></image></view><button class="regist" disabled="{{login_btn}}" data-event-opts="{{[['tap',[['Login',['$event']]]]]}}" bindtap="__e">登录</button><view class="agreement_wrap"><text class="{{[choose]}}">√</text><text data-event-opts="{{[['tap',[['AgreeChange',['$event']]]]]}}" class="read_aggre" bindtap="__e">{{pass}}</text></view></view>