	line-height: 88rpx;
}
.agreement_wrap{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.read_aggre{
	font-size: 24rpx;
	color: #808080;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	line-height: 24rpx;
}
.choose_true{
	width: 20rpx;
	height: 20rpx;
	color: white;
	background: #00adef;
	line-height: 20rpx;
	text-align: center;
	margin-right: 10rpx;
	font-size: 24rpx;
	border: 2rpx solid #00adef;
	font-family: 宋体;
}
.choose_false{
	width: 20rpx;
